    "es.upv.paella.playPauseButton": {
      "enabled": true,
      "order": 0,
      "description": "Play and pause button"
    },
    "es.upv.paella.frameControlButtonPlugin": {
      "enabled": true,
      "side": "right",
      "order": 102
    },
    "es.upv.paella.fullscreenButton": {
      "enabled": true,
      "side": "right",
      "order": 103
    },
    "es.upv.paella.layoutSelector": {
      "enabled": true,
      "side": "right",
      "order": 101,
      "parentContainer": "videoContainer"
